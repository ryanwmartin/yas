        <!--Sec Title-->
        <div class="sec-title centered">
            <div class="icon-box"> <%= image_tag "icons/sec-title-bg" %> </div>
            <h1> <span> <%= t('section5-h1') %> </span></h1>
            <div class="text"> <%= t('section5-text1') %></div>
